<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="BuildingBase">
		<defName>AB_GallatrossBones</defName>
		<label>gallatross bones</label>
		<description>This ribcage once belonged to a living Gallatross of truly exceptional size. Note: if you have the Rim of Madness - Bones mod installed, you can deconstruct these into useable bones.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/AB_Bones1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(4,4)</drawSize>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<rotatable>false</rotatable>

		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<Mass>4</Mass>
			<Flammability>1.0</Flammability>
			<MarketValue>50</MarketValue>
			<WorkToBuild>2000</WorkToBuild>
		</statBases>
		<fillPercent>0.5</fillPercent>
		<destroyable>true</destroyable>
		<useHitPoints>true</useHitPoints>
		<tickerType>Normal</tickerType>
		<selectable>true</selectable>
		<size>(4,4)</size>

		<blockPlants>true</blockPlants>

	</ThingDef>

