<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 

  <ThingDef ParentName="BuildingNaturalBase">
    <defName>AB_GallatrossBones</defName>
    <label>gallatross bones</label>
    <description>This ribcage once belonged to a living Gallatross of truly exceptional size.</description>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Things/Building/AB_Bones1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
    </graphicData>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
	<rotatable>false</rotatable>
	
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Mass>4</Mass>
      <Flammability>1.0</Flammability>
      <MarketValue>50</MarketValue>
    </statBases>
    <destroyable>true</destroyable>
    <useHitPoints>true</useHitPoints>
    <tickerType>Normal</tickerType>
    <selectable>true</selectable>
    <size>(4,4)</size>
   
    <blockPlants>true</blockPlants>
   
  </ThingDef>
  
