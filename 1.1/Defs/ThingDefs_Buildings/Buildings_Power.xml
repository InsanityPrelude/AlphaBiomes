<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 



  <ThingDef ParentName="BuildingBase">
    <defName>AB_PropanePump</defName>
    <label>propane pump</label>
    <description>Extracts propane from a liquid propane lake and refines it into chemfuel. This obviously won't work in most biomes.</description>
    <thingClass>Building</thingClass>
    <graphicData>
      <texPath>Things/Building/AB_PropanePump</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4,4)</drawSize>
      <shadowData>
        <volume>(2.6,1,2.16)</volume>
        <offset>(-0.20,0,0)</offset>
      </shadowData>
      <damageData>
        <rectN>(0.1,1.2,4.75,3.25)</rectN>
        <rectE>(1.25,0.2,3.5,4.7)</rectE>
        <rectS>(0.2,1.2,4.7,3.25)</rectS>
        <rectW>(1.25,0.2,3.5,4.7)</rectW>
      </damageData>
    </graphicData>
    <castEdgeShadows>false</castEdgeShadows>
    <staticSunShadowHeight>0</staticSunShadowHeight>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <fillPercent>1</fillPercent>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
	<defaultPlacingRot>East</defaultPlacingRot>
    <statBases>
      <MaxHitPoints>400</MaxHitPoints>
      <WorkToBuild>2000</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>-20</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(2,2)</size>
    <costList>
      <Steel>150</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
	<uiIconScale>0.75</uiIconScale>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>150</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
	   <li Class="CompProperties_Spawner">
        <requiresPower>true</requiresPower>
        <thingToSpawn>Chemfuel</thingToSpawn>
        <spawnCount>25</spawnCount>
        <spawnIntervalRange><min>200000</min><max>300000</max></spawnIntervalRange>
        <writeTimeLeftToSpawn>true</writeTimeLeftToSpawn>
        <showMessageIfOwned>true</showMessageIfOwned>
      </li>
	 
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <rotatable>true</rotatable>
    <terrainAffordanceNeeded></terrainAffordanceNeeded> <!-- handled by the placeworker -->
    <designationCategory>Power</designationCategory>
   <placeWorkers>
      <li>AlphaBiomes.PlaceWorker_PropanePump</li>
    </placeWorkers>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
    </building>
    
    <researchPrerequisites><li>BiofuelRefining</li></researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>
  
</Defs>
